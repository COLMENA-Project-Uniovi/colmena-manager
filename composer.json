{
    "name": "cakephp/app",
    "description": "CakePHP skeleton app",
    "homepage": "http://cakephp.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "cakephp/cakephp": "4.2.x",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/plugin-installer": "*",
        "dompdf/dompdf": "^0.8.0",
        "mailgun/mailgun-php": "^2.8",
        "tijsverkoyen/css-to-inline-styles": "^2.2",
        "gregwar/image": "2.*",
        "kriswallsmith/buzz": "^1.0",
        "nyholm/psr7": "^1.1",
        "tinify/tinify": "^1.5",
        "friendsofcake/cakepdf": "^4.1",
        "narendravaghela/cakephp-mailgun": "^5.0",
        "cakephp/migrations": "^3.1"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "phpunit/phpunit": "^8.0",
        "cakephp/debug_kit": "^4.0",
        "cakephp/bake": "~1.0"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Neo\\BlogManager\\": "./plugins/Neo/BlogManager/src",
            "Neo\\UsersManager\\": "./plugins/Neo/UsersManager/src",
            "Neo\\ButtonsManager\\": "./plugins/Neo/ButtonsManager/src",
            "Neo\\DistributorsManager\\": "./plugins/Neo/DistributorsManager/src",
            "Neo\\QuestionsManager\\": "./plugins/Neo/QuestionsManager/src",
            "Neo\\ECommerceManager\\": "./plugins/Neo/ECommerceManager/src",
            "Neo\\EventsManager\\": "./plugins/Neo/EventsManager/src",
            "Neo\\FormsManager\\": "./plugins/Neo/FormsManager/src",
            "Neo\\MediaManager\\": "./plugins/Neo/MediaManager/src",
            "Neo\\PartsManager\\": "./plugins/Neo/PartsManager/src",
            "Neo\\CatalogManager\\": "./plugins/Neo/CatalogManager/src",
            "Neo\\TemplatesManager\\": "./plugins/Neo/TemplatesManager/src",
            "Neo\\SectionsManager\\": "./plugins/Neo/SectionsManager/src",
            "Neo\\SubscribersManager\\": "./plugins/Neo/SubscribersManager/src",
            "Neo\\ExternalApiManager\\": "./plugins/Neo/ExternalApiManager/src",
            "Neo\\WebUsersManager\\": "./plugins/Neo/WebUsersManager/src",
            "Neo\\SchedulesManager\\": "./plugins/Neo/SchedulesManager/src/",
            "Neo\\MenuManager\\": "./plugins/Neo/MenuManager/src/",
            "Neo\\ReservationsManager\\": "./plugins/Neo/ReservationsManager/src/",
            "Neo\\MailManager\\": "./plugins/Neo/MailManager/src/",
            "Neo\\MailingManager\\": "./plugins/Neo/MailingManager/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Neo\\BlogManager\\Test\\": "./plugins/Neo/BlogManager/tests",
            "Neo\\ButtonsManager\\Test\\": "./plugins/Neo/ButtonsManager/tests",
            "Neo\\DistributorsManager\\Test\\": "./plugins/Neo/DistributorsManager/tests",
            "Neo\\QuestionsManager\\Test\\": "./plugins/Neo/QuestionsManager/tests",
            "Neo\\ECommerceManager\\Test\\": "./plugins/Neo/ECommerceManager/tests",
            "Neo\\EventsManager\\Test\\": "./plugins/Neo/EventsManager/tests",
            "Neo\\FormsManager\\Test\\": "./plugins/Neo/FormsManager/tests",
            "Neo\\MediaManager\\Test\\": "./plugins/Neo/MediaManager/tests",
            "Neo\\PartsManager\\Test\\": "./plugins/Neo/PartsManager/tests",
            "Neo\\CatalogManager\\Test\\": "./plugins/Neo/CatalogManager/tests",
            "Neo\\TemplatesManager\\Test\\": "./plugins/Neo/TemplatesManager/tests",
            "Neo\\SectionsManager\\Test\\": "./plugins/Neo/SectionsManager/tests",
            "Neo\\SubscribersManager\\Test\\": "./plugins/Neo/SubscribersManager/tests",
            "Neo\\ExternalApiManager\\Test\\": "./plugins/Neo/ExternalApiManager/tests",
            "Neo\\WebUsersManager\\Test\\": "./plugins/Neo/WebUsersManager/tests",
            "Neo\\SchedulesManager\\Test\\": "./plugins/Neo/SchedulesManager/tests/",
            "Neo\\ReservationsManager\\Test\\": "./plugins/Neo/ReservationsManager/tests/",
            "Neo\\MailManager\\Test\\": "./plugins/Neo/MailManager/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
